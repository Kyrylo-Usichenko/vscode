{
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "background.enabled": true,

    "background.fullscreen": {
        "images": [
            "https://r4.wallpaperflare.com/wallpaper/448/174/357/neon-4k-hd-best-for-desktop-wallpaper-b896bd6800903c48d09c01fed852c41a.jpg"
        ], // urls of your images
        "opacity": 0.91, // 0.85 ~ 0.95 recommended
        "size": "cover", // also css, `cover` to self-adaption (recommended)，or `contain`、`200px 200px`
        "position": "center", // alias to `background-position`, default `center`
        "interval": 0 // seconds of interval for carousel, default `0` to disabled.
    },
    "editor.accessibilitySupport": "off",
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit",
        "source.organizeImports": "explicit"
    },
    "editor.cursorBlinking": "solid",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorStyle": "block",
    "editor.fontFamily": "'JetBrainsMono Nerd Font'",
    "editor.fontLigatures": true,
    "editor.fontSize": 18,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "editor.lineHeight": 26,
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": false,
    "editor.renderLineHighlight": "gutter",
    "editor.renderWhitespace": "all",
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.suggest.insertMode": "replace",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80,
    "errorLens.enabled": true,
    "errorLens.enabledDiagnosticLevels": ["warning", "info", "hint", "error"],
    "eslint.codeActionsOnSave.rules": [],
    "eslint.format.enable": true,
    "explorer.compactFolders": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.autoSave": "afterDelay",
    "files.eol": "\n",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "gitlens.currentLine.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "indentRainbow.colorOnWhiteSpaceOnly": true,
    "indentRainbow.colors": [
        "rgba(255,255,64,0.2)",
        "rgba(127,255,127,0.2)",
        "rgba(255,127,255,0.2)",
        "rgba(79,236,236,0.2)"
    ],
    "indentRainbow.indicatorStyle": "light",
    "indentRainbow.lightIndicatorStyleLineWidth": 1,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "prettier.bracketSameLine": false,

    "prettier.bracketSpacing": true,
    "prettier.endOfLine": "lf",
    "prettier.jsxSingleQuote": true,
    "prettier.printWidth": 100,
    "prettier.quoteProps": "consistent",
    "prettier.semi": true,
    "prettier.singleAttributePerLine": true,
    "prettier.singleQuote": false,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "none",
    "prettier.useTabs": false,
    "redhat.telemetry.enabled": true,
    "security.workspace.trust.untrustedFiles": "open",
    "svelte.enable-ts-plugin": true,
    "tailwind-fold.foldStyle": "QUOTES",
    "tailwind-fold.showTailwindImage": false,
    "tailwind-fold.unfoldIfLineSelected": true,
    "tailwindCSS.emmetCompletions": true,
    "terminal.explorerKind": "external",
    "terminal.external.windowsExec": "",
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "update.showReleaseNotes": false,
    "window.zoomLevel": 1,
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#0a84fd1d",
        "editor.lineHighlightBorder": "#0073ff2e",
        "editorLineNumber.activeForeground": "#ffffffdc"
    },
    "workbench.colorTheme": "Dracula",
    "workbench.editor.showTabs": "single",
    "workbench.editorAssociations": {
        "*.db": "default"
    },
    "workbench.iconTheme": "vscode-icons",
    "diffEditor.ignoreTrimWhitespace": false,
    "diffEditor.hideUnchangedRegions.enabled": true,
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
